  "onboarding": {
    "welcome": {
      "title": "أهلاً بك",
      "description": "مرحباً بك في برنامجنا لإدارة المبيعات، التصنيع، الحسابات، الفواتير والمخازن."
    },
    "sales": {
      "title": "المبيعات",
      "description": "تسجيل العملاء، إنشاء عروض الأسعار، متابعة الطلبات، والتقارير اليومية والشهرية والسنوية."
    },
    "manufacturing": {
      "title": "التصنيع",
      "description": "إدارة أوامر التصنيع، تحديد المواد الخام، متابعة الإنتاج، وتقارير كفاءة العمال والآلات."
    },
    "accounting": {
      "title": "الحسابات",
      "description": "تسجيل الإيرادات والمصروفات، إدارة الحسابات البنكية والكاش، كشف حساب العملاء والموردين، والتقارير المالية."
    },
    "invoices": {
      "title": "الفواتير",
      "description": "فواتير المبيعات والمشتريات والصيانة والتصنيع، دفع وتحصيل الفواتير، وأرشيف قابل للبحث والطباعة."
    },
    "inventory": {
      "title": "المخازن",
      "description": "تسجيل المواد الخام والمنتجات الجاهزة، متابعة الكميات، تنبيه انخفاض المخزون، وجرد المخازن."
    }
  },
